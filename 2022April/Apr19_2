package ws;

public abstract class Car {

private String name;
private double fe;  //Fuel-efficient
private String color;
private double volume; // 배기량
private double cf; // CurrentFuel

public Car() {
}

public Car(String name, double fe, String color, double volume, double cf) {
	this.name = name;
	this.fe = fe;
	this.color = color;
	this.volume = volume;
	this.cf = cf;
}

public String getName() {
	return name;
}

public void setName(String name) {
	this.name = name;
}

public double getFe() {
	return fe;
}

public void setFe(double fe) {
	this.fe = fe;
}

public String getColor() {
	return color;
}

public void setColor(String color) {
	this.color = color;
}

public double getVolume() {
	return volume;
}

public void setVolume(double volume) {
	this.volume = volume;
}

public double getCf() {
	return cf;
}

public void setCf(double cf) {
	this.cf = cf;
}

public void go() {	
}
public void stop() {
}
public void alarm() {
}
public void maxspeed() {
}






@Override
public String toString() {
	return "Car [name=" + name + ", fe=" + fe + ", color=" + color + ", volume=" + volume + ", cf=" + cf + "]";
}
 


}
