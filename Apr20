package ws;

import P346.CustomerVO;

public interface Inter {
	public default void connect() {
		System.out.println("Connect...");
	}
	public default void close() {
		System.out.println("Close...");
	}
	
	public void create(CustomerVO c);
	public void delete(String id);
	public void update(CustomerVO c);
	public CustomerVO read(String id);
}
package ws;

public class CustomerVO {
	private String id;
	private String pwd;
	private String name;
	
	public CustomerVO() {
	}

	public CustomerVO(String id, String pwd, String name) {
		this.id = id;
		this.pwd = pwd;
		this.name = name;
	}

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getPwd() {
		return pwd;
	}

	public void setPwd(String pwd) {
		this.pwd = pwd;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	@Override
	public String toString() {
		return "CustomerVO [id=" + id + ", pwd=" + pwd + ", name=" + name + "]";
	}
	
}
package ws;

import java.util.HashMap;

import P346.CustomerVO;

public class Oracle implements Inter {
HashMap<String, CustomerVO> map;

	public Oracle() {
		map = new HashMap<String, CustomerVO>();
	}

	@Override
	public void create(CustomerVO c) {
		// TODO Auto-generated method stub
		String key = c.getId();
		map.put(key, c);
	}

	@Override
	public void delete(String id) {
		// TODO Auto-generated method stub
		map.remove(id);
	}

	@Override
	public void update(CustomerVO c) {
		// TODO Auto-generated method stub
		String key = c.getId();
		map.put(key, c);
		
	}

	@Override
	public CustomerVO read(String id) {
		// TODO Auto-generated method stub
		CustomerVO c1 = null;
		c1 = map.get(id);
		return c1; 
	}


}
