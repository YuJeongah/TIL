Jquery 


<meta charset="UTF-8">


<script>

$(document).ready(function(){
	$('#box').css({
		'width' : '100px',
		'height' : '100px',
		'background' : 'red'
		
	}).animate({
		'width' : '500px',
		'opacity' : '0.6'
	},500);
	
});

</script>


<h1>JQ10 Main</h1>

<div id ="box" ></div>

